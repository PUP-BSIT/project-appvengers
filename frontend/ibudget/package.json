{
  "name": "ibudget",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "test:watch": "ng test --code-coverage",
    "test:coverage": "ng test --code-coverage --watch=false --browsers=ChromeHeadless && start coverage/index.html",
    "test:no-coverage": "ng test --watch=false --browsers=ChromeHeadless"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@ng-icons/bootstrap-icons": "^32.4.0",
    "@ng-icons/core": "^32.4.0",
    "@ngx-loading-bar/core": "^7.0.0",
    "@ngx-loading-bar/http-client": "^7.0.0",
    "@ngx-loading-bar/router": "^7.0.0",
    "@types/dompurify": "^3.0.5",
    "angular-svg-round-progressbar": "^15.0.0",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "chart.js": "^4.5.1",
    "dompurify": "^3.3.0",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "marked": "^17.0.1",
    "ng2-charts": "^8.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.4",
    "@angular/build": "^20.3.4",
    "@angular/cli": "^20.3.13",
    "@angular/compiler-cli": "^20.3.0",
    "@playwright/test": "^1.56.1",
    "@types/bootstrap": "^5.2.10",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.10.1",
    "@types/xlsx": "^0.0.35",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
